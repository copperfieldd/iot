<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="MySQLTables" targetRuntime="MyBatis3" defaultModelType="flat">

        <commentGenerator>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/bytebpm" userId="root"
                        password="bytecloud2017">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.bytebpm.model.mybatis.gen"
                            targetProject="src/main/java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis"
                         targetProject="src/main/resources/META-INF">
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.bytebpm.repository.mybatis"
                             targetProject="src/main/unused">
        </javaClientGenerator>

        <table tableName="t_user" domainObjectName="TUser"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
            <generatedKey column="id" identity="true" sqlStatement="SELECT LAST_INSERT_ID()"/>
            <columnOverride column="create_time" javaType="java.lang.String"
                            typeHandler="com.bytebpm.util.R2DateTimeHandler"/>
            <columnOverride column="update_time" javaType="java.lang.String"
                            typeHandler="com.bytebpm.util.R2DateTimeHandler"/>
        </table>

		<table tableName="m_acl" domainObjectName="MAcl"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
            <generatedKey column="id" identity="true" sqlStatement="SELECT LAST_INSERT_ID()"/>
            <columnOverride column="create_time" javaType="java.lang.String"
                            typeHandler="com.bytebpm.util.R2DateTimeHandler"/>
            <columnOverride column="update_time" javaType="java.lang.String"
                            typeHandler="com.bytebpm.util.R2DateTimeHandler"/>
            <columnOverride column="type" javaType="com.bytebpm.model.enums.AclType"
                          typeHandler="com.bytebpm.util.R2EHandler"/>
        </table>
        
        <table tableName="m_role" domainObjectName="MRole"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
            <generatedKey column="id" identity="true" sqlStatement="SELECT LAST_INSERT_ID()"/>
            <columnOverride column="create_time" javaType="java.lang.String"
                            typeHandler="com.bytebpm.util.R2DateTimeHandler"/>
            <columnOverride column="update_time" javaType="java.lang.String"
                            typeHandler="com.bytebpm.util.R2DateTimeHandler"/>
        </table>
        
    </context>
</generatorConfiguration>